    "qs": "^6.7.0",
    "request": "^2.88.0",
    "winston": "^3.2.1"
  },
  "typings": "couchbase",
  "devDependencies": {
    "chai": "^4.2.0",
    "jshint": "^2.10.2",
    "mocha": "^6.1.4",
    "prebuild": "^9.0.0",
    "uuid": "^3.3.2"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/corbinu/couchnode.git"
  },
  "config": {
