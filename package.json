{
  "description": "The unofficial Couchbase Node.js Client Library.",
  "engines": {
    "node": ">=4.0.0"
  },
  "version": "2.3.0",
  "keywords": [
    "couchbase",
    "libcouchbase",
    "memcached",
    "nosql",
    "json",
    "document"
  ],
  "main": "./lib/couchbase",
  "license": "Apache-2.0",
  "name": "@corbinu/couchbase",
  "dependencies": {
    "@types/node": "^7.0.0",
    "bindings": "^1.2.1",
    "nan": "^2.0.8",
    "prebuild-install": "^2.1.0",
    "request": "^2.61.0"
  },
  "typings": "couchbase",
  "devDependencies": {
    "prebuild": "^6.0.0",
    "jshint": "^2.8.0",
    "mocha": "^3.2.0"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/corbinu/couchnode.git"
  },
  "config": {
    "unsafe-perm": true
  },
  "scripts": {
    "install": "prebuild-install || node-gyp rebuild",
    "test": "mocha",
    "rebuild": "prebuild --compile",
    "prebuild": "prebuild --verbose --strip"
  }
}
