{
  "description": "The unofficial Couchbase Node.js Client Library.",
  "engines": {
    "node": ">=0.10.0"
  },
  "version": "2.1.5",
  "keywords": [
    "couchbase",
    "libcouchbase",
    "memcached",
    "nosql",
    "json",
    "document"
  ],
  "main": "./lib/couchbase",
  "license": "Apache2",
  "name": "@corbinu/couchbase",
  "dependencies": {
    "bindings": "~1.2.1",
    "jsonparse": "~1.0.0",
    "nan": "~2.0.8",
    "request": "~2.61.0",
    "node-pre-gyp": "^0.6.10"
  },
  "bundledDependencies": [
    "node-pre-gyp"
  ],
  "devDependencies": {
    "aws-sdk": "^2.2.0",
    "ink-docstrap": "git+https://github.com/brett19/docstrap.git#master",
    "istanbul": "~0.3.19",
    "jsdoc": "~3.3.2",
    "jsdoc-stability-tag": "~1.0.0",
    "jshint": "~2.8.0",
    "mocha": "~2.3.0"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/corbinu/couchnode.git"
  },
  "config": {
    "unsafe-perm": true
  },
  "scripts": {
    "test": "./node_modules/mocha/bin/mocha",
    "install": "node-pre-gyp install --fallback-to-build"
  },
  "binary": {
    "module_name": "couchbase_impl",
    "module_path": "./lib/binding/",
    "host": "https://couchnode.decimal.io",
    "remote_path": "./v{version}",
    "package_name": "{node_abi}-{platform}-{arch}.tar.gz"
  }
}
