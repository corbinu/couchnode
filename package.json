{
    "description": "The unofficial Couchbase Node.js Client Library.",
    "engines": {
        "node": ">=4.0.0"
    },
    "version": "2.2.5",
    "keywords": [
        "couchbase",
        "libcouchbase",
        "memcached",
        "nosql",
        "json",
        "document"
    ],
    "main": "./lib/couchbase",
    "license": "Apache-2.0",
    "name": "@corbinu/couchbase",
    "dependencies": {
        "@types/node": "^6.0.52",
        "bindings": "^1.2.1",
        "nan": "^2.0.8",
        "prebuild": "^5.1.2",
        "request": "^2.61.0"
    },
    "typings": "couchbase",
    "devDependencies": {
        "ink-docstrap": "git+https://github.com/brett19/docstrap.git#master",
        "istanbul": "^0.4.2",
        "jsdoc": "^3.3.2",
        "jsdoc-stability-tag": "^1.0.0",
        "jshint": "^2.8.0",
        "mocha": "^3.2.0"
    },
    "repository": {
        "type": "git",
        "url": "http://github.com/corbinu/couchnode.git"
    },
    "config": {
        "unsafe-perm": true
    },
    "scripts": {
        "install": "prebuild --install",
        "test": "mocha",
        "rebuild": "prebuild --compile",
        "prebuild": "prebuild --verbose --strip"
    }
}