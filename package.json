{
  "description": "The unofficial Couchbase Node.js Client Library.",
  "engines": {
    "node": ">=8.9"
  },
  "version": "2.5.4",
  "keywords": [
    "couchbase",
    "libcouchbase",
    "memcached",
    "nosql",
    "json",
    "document"
  ],
  "main": "./lib/couchbase",
  "license": "Apache-2.0",
  "name": "@corbinu/couchbase",
  "dependencies": {
    "@types/node": "^10.12.10",
    "bindings": "~1.3.0",
    "nan": "^2.11.1",
    "prebuild-install": "^5.2.1",
    "request": "^2.88.0",
    "winston": "^3.1.0",
    "winston-null": "^1.0.2"
  },
  "typings": "couchbase",
  "devDependencies": {
    "jshint": "^2.9.6",
    "mocha": "^5.2.0",
    "prebuild": "^8.1.2"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/corbinu/couchnode.git"
  },
  "config": {
    "unsafe-perm": true
  },
  "scripts": {
    "install": "prebuild-install || node-gyp rebuild",
    "test": "mocha",
    "rebuild": "prebuild --compile",
    "prebuild": "prebuild --verbose --strip"
  }
}
